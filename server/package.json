{
  "name": "server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development ts-node-dev --respawn --transpile-only ./src/index.ts",
    "serve": "NODE_ENV=production node build/index.js",
    "build": "ncc build src/index.ts --minify --no-cache --out build"
  },
  "dependencies": {
    "express": "^4.17.3",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.21",
    "@vercel/ncc": "^0.33.3",
    "ts-node-dev": "^1.1.8"
  }
}
